{
  "paths": {
    "raw": "data/raw",
    "processed": "data/processed"
  },
  "dataset": {
    "bug_reports_glob": "data/raw/**/bug_reports/*.json",
    "screenshots_glob": "data/raw/**/screenshots/**/*.*",
    "ui_logs_glob": "data/raw/**/ui_logs/*.jsonl"
  },
  "llm": {
    "provider": "openai",
    "model": "gpt-4o-mini",
    "api_key_env": "OPENAI_API_KEY",
    "temperature": 0.2
  }
}