                    "\\t\\t\\t\\t'entity': /\\b[A-Z][A-Za-z0-9_]*\\b/",
                    "\\t\\t\\t}",
                    "\\t\\t}",
                    "\\t};",
                    "",
                    "\\t_self.Prism = _;"
                ]
            }
        ],
        "components/index.js": [
            {
                "SEARCH": [
                    "\\t\\tconst pathToLanguage = './prism-' + lang;"
                ],
                "REPLACE": [
                    "\\t\\tconst pathToLanguage = lang === 'mermaid' ? './prism-mermaid' : './prism-' + lang;"
                ]
            }
        ]
    },
