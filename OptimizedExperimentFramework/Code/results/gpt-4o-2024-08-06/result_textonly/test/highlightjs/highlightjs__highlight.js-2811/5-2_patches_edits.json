{
    "1/1": {
        "src/languages/python.js": [
            {
                "SEARCH": [
                    "{",
                    "className: 'meta',",
                    "begin: /^[\\t ]*@/, end: /$/",
                    "},"
                ],
                "REPLACE": [
                    "{",
                    "className: 'meta',",
                    "begin: /^[\\t ]*@/,",
                    "end: /(?=#|$)/, // End at the start of a comment or end of line",
                    "contains: [hljs.HASH_COMMENT_MODE] // Include comment mode",
                    "},"
                ]
            }
        ]
    },
