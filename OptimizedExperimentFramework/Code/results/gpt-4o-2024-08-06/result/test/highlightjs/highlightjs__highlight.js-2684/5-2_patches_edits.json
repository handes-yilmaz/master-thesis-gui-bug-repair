{
    "1/1": {
        "src/languages/bash.js": [
            {
                "SEARCH": [
                    "const SUBST = {",
                    "className: 'subst',",
                    "begin: /\\$\\(/, end: /\\)/,",
                    "contains: [hljs.BACKSLASH_ESCAPE]",
                    "};",
                    "const QUOTE_STRING = {",
                    "className: 'string',",
                    "begin: /\"/, end: /\"/,",
                    "contains: [",
                    "hljs.BACKSLASH_ESCAPE,",
                    "VAR,",
                    "SUBST",
                    "]",
                    "};"
                ],
                "REPLACE": [
                    "const SUBST = {",
                    "className: 'subst',",
                    "begin: /\\$\\(/, end: /\\)/,",
                    "contains: [hljs.BACKSLASH_ESCAPE]",
                    "};",
                    "const QUOTE_STRING = {",
                    "className: 'string',",
                    "begin: /\"/, end: /\"/,",
                    "contains: [",
                    "hljs.BACKSLASH_ESCAPE,",
                    "VAR,",
                    "SUBST",
                    "]",
                    "};",
                    "",
                    "const PIPE = {",
                    "className: 'operator',",
                    "begin: /\\|/",
                    "};",
                    "",
                    "const REDIRECTION = {",
                    "className: 'operator',",
                    "begin: />|<|>>|<<|<>|>&|<&|<>>|<<</",
                    "};",
                    "",
                    "const HEREDOC = {",
                    "className: 'string',",
                    "begin: /<<-?\\s*(\\w+)\\b/, end: /\\b\\1\\b/,",
                    "contains: [hljs.BACKSLASH_ESCAPE]",
                    "};",
                    "",
                    "const CONTINUATION = {",
                    "className: 'symbol',",
                    "begin: /\\\\$/",
                    "};"
                ]
            }
        ]
    },
